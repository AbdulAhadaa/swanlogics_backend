{
  "version": 2,
  "public": true,
  "functions": {
    "api/*.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/chat",
      "destination": "/api/chat"
    },
    {
      "source": "/contact",
      "destination": "/api/contact"
    },
    {
      "source": "/quote",
      "destination": "/api/quotation"
    },
    {
      "source": "/api-docs",
      "destination": "/api/docs"
    },
    {
      "source": "/api-docs/(.*)",
      "destination": "/api/docs"
    }
  ]
}